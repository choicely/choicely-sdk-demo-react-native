{
  "name": "choicely-rn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "web": "webpack-dev-server --config ./web/webpack.config.js --mode development",
    "bundle:android": "mkdir -p dist/android && react-native bundle --platform android --dev false --entry-file src/index.js --assets-dest dist/android --bundle-output dist/android/index.android.bundle",
    "bundle:ios": "mkdir -p dist/ios && react-native bundle --platform ios --dev false --entry-file src/index.js --assets-dest dist/ios --bundle-output dist/ios/main.jsbundle",
    "bundle:web": "mkdir -p dist/web && webpack --no-stats --no-watch --watch-options-stdin --no-color --no-devtool --config ./web/webpack.config.js --mode production --output-path ./dist/web",
    "bundle:all": "set -euo pipefail && npm run bundle:android && npm run bundle:ios && npm run bundle:web"
  },
  "dependencies": {
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/native-stack": "^7.6.2",
    "@shopify/flash-list": "^2.2.0",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "process": "^0.11.10",
    "react": "19.1.1",
    "react-native": "0.82.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-mmkv": "^4.0.1",
    "react-native-nitro-modules": "^0.31.10",
    "react-native-permissions": "^5.4.4",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.18.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-video": "^6.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-transform-modules-commonjs": "^7.24.8",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.0",
    "@react-native/metro-config": "0.82.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-react-native-web": "^0.21.2",
    "concurrently": "^9.2.1",
    "html-webpack-plugin": "^5.6.0",
    "http-proxy": "^1.18.1",
    "http-server": "^14.1.1",
    "metro-cache": "^0.83.3",
    "react-dom": "19.1.1",
    "react-native-web": "^0.21.2",
    "react-test-renderer": "19.1.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "engines": {
    "node": ">=20"
  }
}
